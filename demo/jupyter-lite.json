{
  "jupyter-config-data": {
    "enableMemoryStorage": true,
    "contentsStorageDrivers": ["memoryStorageDriver"],
    "spectaConfig": {
      "topBar": {
        "title": "Specta Tree"
      },
      "defaultLayout": "default",
      "perFileConfig": {
        "blog.ipynb": {
          "hideTopbar": false,
          "defaultLayout": "article"
        },
        "slides.ipynb": {
          "hideTopbar": true,
          "slidesTheme": "sky"
        }
      },
      "labConfig": {
        "setSingleMode": true,
        "hideLeftPanel": true,
        "hideRightPanel": true,
        "hideStatusbar": true,
        "hideHeader": false
      }
    },
    "disabledExtensions": [
      "@jupyterlab/notebook-extension:open-with-no-kernel",
      "@jupyterlite/application-extension:opener"
    ]
  }
}
